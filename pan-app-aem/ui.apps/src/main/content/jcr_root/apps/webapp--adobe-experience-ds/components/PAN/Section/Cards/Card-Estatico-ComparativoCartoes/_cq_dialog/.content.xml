<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Card-Estatico-ComparativoCartoes"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-variantes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variante"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <colorVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Cores de fundo"
                                name="./colorVariant"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Branco"
                                        value="bg-branco"/>
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        text="Azul"
                                        value="bg-azul"/>
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        text="Preto"
                                        value="bg-preto"/>
                                </items>
                            </colorVariant>
                        </items>
                    </tab-variantes>
                    <tab-textos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Textos"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <PalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Palavra Destaque"
                                name="./palavraDestaque"
                                value="Palavra de destaque"/>
                            <Titulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Título"
                                name="./titulo"
                                value="Exemplo de título com no máximo 3 linhas aqui!"/>
                        </items>
                    </tab-textos>
                    <tab-cards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Cards"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <presets
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <list
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldLabel="Insira os Slides"
                                                composite="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./comparativoCartoes">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <fieldset
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Preencha o card"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <HideLabelCheckbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    fieldDescription="Ocultar a label do componente"
                                                                    text="Ocultar label?"
                                                                    uncheckedValue="{Boolean}false"
                                                                    checked="{Boolean}true"
                                                                    value="{Boolean}true"
                                                                    name="./hideLabelCheckbox" />
                                                                <Label
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    autocomplete="off"
                                                                    autofocus="{Boolean}true"
                                                                    fieldLabel="Label do cartão"
                                                                    name="./firstCardLabel"
                                                                    value="Melhor opção" />
                                                                <Titulo2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    autocomplete="off"
                                                                    autofocus="{Boolean}true"
                                                                    fieldLabel="Título"
                                                                    name="./tituloCard2"
                                                                    value="Sem Anuidade"/>
                                                                <Informacao2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    autocomplete="off"
                                                                    autofocus="{Boolean}true"
                                                                    fieldLabel="Frase informativa"
                                                                    name="./informacao2"
                                                                    value="Renda mínima: salário mínimo"/>
                                                                <Marcadores2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                    fieldLabel="Texto do Card"
                                                                    name="./marcadores2"
                                                                    removeSingleParagraphContainer="{Boolean}true"
                                                                    useFixedInlineToolbar="{Boolean}true"
                                                                    value="#">
                                                                    <rtePlugins jcr:primaryType="nt:unstructured">
                                                                        <format
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="bold,italic"/>
                                                                        <justify
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="-"/>
                                                                        <links
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="modifylink,unlink"/>
                                                                        <lists
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="*"/>
                                                                        <tracklinks
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="*"/>
                                                                        <styles
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="styles">
                                                                            <styles jcr:primaryType="cq:WidgetCollection">
                                                                                <textClass
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    cssName="text-fonte-azul"
                                                                                    text="Estilo Fonte Azul"/>
                                                                                <textClass2
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    cssName="text-fonte-branco"
                                                                                    text="Estilo Fonte Branco"/>
                                                                                <textClass3
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    cssName="text-fonte-preto"
                                                                                    text="Estilo Fonte Preto" />
                                                                                <textClass4
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    cssName="text-fonte-neutral-dark"
                                                                                    text="Estilo Fonte Neutral Dark" />
                                                                                <textClass5
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    cssName="text-fonte-body-2"
                                                                                    text="Estilo Fonte body-2"/>
                                                                            </styles>
                                                                        </styles>
                                                                    </rtePlugins>
                                                                    <uiSettings jcr:primaryType="nt:unstructured">
                                                                        <cui jcr:primaryType="nt:unstructured">
                                                                            <inline
                                                                                jcr:primaryType="nt:unstructured"
                                                                                toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                                                                <popovers jcr:primaryType="nt:unstructured">
                                                                                    <justify
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                                                        ref="justify"/>
                                                                                    <lists
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                                        ref="lists"/>
                                                                                </popovers>
                                                                            </inline>
                                                                        </cui>
                                                                    </uiSettings>
                                                                </Marcadores2>
                                                                <HideDiscalimerCheckbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    fieldDescription="Ocultar o disclaimer do componente"
                                                                    text="Ocultar disclaimer?"
                                                                    uncheckedValue="{Boolean}false"
                                                                    checked="{Boolean}true"
                                                                    value="{Boolean}true"
                                                                    name="./hideDisclaimerCheckbox" />
                                                                <Disclaimer
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                    fieldLabel="Disclaimer"
                                                                    name="./disclaimer"
                                                                    removeSingleParagraphContainer="{Boolean}true"
                                                                    useFixedInlineToolbar="{Boolean}true"
                                                                    value="{String}*Benefício de desconto na rede Cinemark, exclusivo da bandeira Elo.">
                                                                    <rtePlugins
                                                                        jcr:primaryType="nt:unstructured">
                                                                        <format
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="bold,italic" />
                                                                        <justify
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="-" />
                                                                        <links
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="modifylink,unlink" />
                                                                        <lists
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="*" />
                                                                        <tracklinks
                                                                            jcr:primaryType="nt:unstructured"
                                                                            features="*" />
                                                                    </rtePlugins>
                                                                    <uiSettings
                                                                        jcr:primaryType="nt:unstructured">
                                                                        <cui
                                                                            jcr:primaryType="nt:unstructured">
                                                                            <inline
                                                                                jcr:primaryType="nt:unstructured"
                                                                                toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                                                                <popovers
                                                                                    jcr:primaryType="nt:unstructured">
                                                                                    <justify
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                                                        ref="justify" />
                                                                                    <lists
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                                        ref="lists" />
                                                                                </popovers>
                                                                            </inline>
                                                                        </cui>
                                                                    </uiSettings>
                                                                </Disclaimer>
                                                            </items>
                                                        </fieldset>
                                                    </items>
                                                </field>
                                            </list>
                                        </items>
                                    </column>
                                </items>
                            </presets>
                        </items>
                    </tab-cards>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="ID da Seção"
                                name="./sectionID"
                                value=""/>
                            <tagPalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag da Palavra destaque"
                                name="./tagPalavraDestaque">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </tagPalavraDestaque>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do titulo"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </tagTitulo>
                        </items>
                    </tab-seo>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>
