<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Formulario-Simulador-FGTS-Imagem"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-variantes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variantes"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <colorVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Cores de fundo"
                                name="./colorVariant"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Branco"
                                        value="bg-branco"/>
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        text="Cinza"
                                        value="bg-cinza"/>
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        text="Azul"
                                        value="bg-azul"/>
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        text="Preto"
                                        value="bg-preto"/>
                                </items>
                            </colorVariant>
                        </items>
                    </tab-variantes>
                    <tab-textos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Textos"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <SubTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Subtítulo (máximo 40 caracteres)"
                                maxlength="{Long}40"
                                name="./subtitulo"
                                value="Subtítulo com 1 linha aqui!"/>
                            <Titulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Titulo (máximo 56 caracteres)"
                                maxlength="{Long}56"
                                name="./titulo"
                                value="Título com 2 linhas aqui!"/>
                            <Disclaimer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Disclaimer (máximo 200 caracteres)"
                                maxlength="{Long}200"
                                name="./disclaimer"
                                value="Disclaimer aqui!"/>
                        </items>
                    </tab-textos>
                    <tab-botoes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Botões"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <Tabs-botoes
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                                <items jcr:primaryType="nt:unstructured">
                                    <tab-botao
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Botão"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <TextoBotaoMobile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                autofocus="{Boolean}true"
                                                fieldLabel="Texto botão mobile (máximo de 20 caracteres)"
                                                maxlength="{Long}20"
                                                name="./textMobileButton"
                                                value="Texto Mobile"/>
                                            <TextoBotaoDesktop
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                autofocus="{Boolean}true"
                                                fieldLabel="Texto botão desktop (máximo de 30 caracteres)"
                                                maxlength="{Long}30"
                                                name="./textDesktopButton"
                                                value="Texto Desktop"/>
                                            <DataTrack
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Data Track botão (máximo de 100 caracteres)"
                                                maxlength="{Long}100"
                                                name="./ButtonButtonDataTracktaTrack"
                                                value="#"/>
                                            <Href
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Link do arquivo ou página."
                                                name="./ButtonHref"
                                                value="#"/>
                                            <Title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Title botão (descrição do link)"
                                                name="./ButtonTitle"
                                                value="#"/>
                                        </items>
                                    </tab-botao>
                                    <tab-link
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Link Apoio"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <linkApoio
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                autofocus="{Boolean}true"
                                                fieldLabel="Link apoio (máximo de 20 caracteres)"
                                                maxlength="{Long}20"
                                                name="./textApoio"
                                                value="Como vejo o saldo?"/>
                                            <DataTrack
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Button Data Track (máximo de 100 caracteres)"
                                                maxlength="{Long}100"
                                                name="./LinkDataTrack"
                                                value="#"/>
                                            <Href
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                autocomplete="off"
                                                fieldLabel="Link do arquivo ou página."
                                                name="./linkHref"
                                                value="#"/>
                                            <Title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Title do link (descrição do link)"
                                                name="./linkTitle"
                                                value="#"/>
                                        </items>
                                    </tab-link>
                                </items>
                            </Tabs-botoes>
                        </items>
                    </tab-botoes>
                    <tab-imagens
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Imagens"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs-imagens
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                                <items jcr:primaryType="nt:unstructured">
                                    <tab-imagem
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Imagem"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <altImagem
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="ALT imagem (texto alternativo)"
                                                maxlength="{Long}50"
                                                name="./altImagem"
                                                value="#"/>
                                            <imageDesktopWebp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Imagem Desktop WEBP (656x560)"
                                                fileNameParameter="./imageDesktopNameParameterWebp"
                                                fileReferenceParameter="./imageDesktopReferenceWebp"
                                                mimeTypes="[image/webp]"
                                                multiple="{Boolean}false"
                                                name="./imageDesktopWebp"
                                                title="Imagem Desktop WEBP"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </tab-imagem>
                                </items>
                            </tabs-imagens>
                        </items>
                    </tab-imagens>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="ID da Seção"
                                maxlength="{Long}10"
                                name="./sectionID"
                                value=""/>
                            <tagSubTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do subtitulo"
                                name="./tagSubTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </tagSubTitulo>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do titulo"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </tagTitulo>
                        </items>
                    </tab-seo>
                    <tab-endpoint
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configurações"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <endpointAPI
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                autocomplete="off"
                                fieldLabel="Endpoint (URL) para consumo/envio de dados (API)"
                                name="./endpointAPI"
                                value="https://ap-gateway-web-dev.bancopan.com.br/institucional/api/simulador/site/emprestimos/fgts/aniversario"/>
                        </items>
                    </tab-endpoint>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>
