<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Section Card Estatico Parceiros (Shopping)"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[webapp--aem-institucional.editor]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-conteudo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Conteúdo"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <PalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Palavra Destaque"
                                name="./palavraDestaque"
                                value="Palavra destaque" />
                            <TituloAntesCashback
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Texto Antes do valor do cashback"
                                name="./tituloAntesCashback"
                                value="Texto Antes do valor do cashback" />
                            <TituloDepoisCashback
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Texto depois do valor do cashback"
                                name="./tituloDepoisCashback"
                                value="Texto depois do valor do cashback" />
                        </items>
                    </tab-conteudo>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="ID da Seção"
                                name="./sectionID"
                                value="" />
                            <tagPalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag da palvra destaque"
                                name="./tagPalavraDestaque">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagPalavraDestaque>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do título"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagTitulo>
                        </items>
                    </tab-seo>
                    <tab-form
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <EndPoint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Endpoint - API"
                                name="./endpointAPI"
                                value="https://ap-gateway-web-hml.bancopan.com.br/institucional/api/site/marketplace/" />
                        </items>
                    </tab-form>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>