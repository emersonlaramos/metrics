<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Nps-Relacional"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[webapp--aem-institucional.editor]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-textos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Textos"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <PalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Palavra Destaque"
                                name="./palavraDestaque"
                                value="Sua opinião importa" />
                            <Titulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Titulo"
                                name="./titulo"
                                value="Queremos saber como foi sua experiência " />
                            <Paragrafo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Parágrafo"
                                name="./paragrafo"
                                useFixedInlineToolbar="{Boolean}true"
                                removeSingleParagraphContainer="{Boolean}true"
                                value="Aproveite essa oportunidade.">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic" />
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-" />
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                    ref="justify" />
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists" />
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </Paragrafo>
                            <tituloPergunta1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Titulo pergunta 1"
                                name="./tituloPergunta1"
                                value="Numa escala de 0 a 10, o quanto você recomendaria o PAN a um amigo ou parente?" />
                            <tituloPergunta2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Titulo pergunta 2"
                                name="./tituloPergunta2"
                                value="Nos conte o motivo de sua nota:" />
                            <Agradecimento
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Titulo de agradecimento"
                                name="./agradecimento"
                                value="Obrigado!" />
                            <Feedback
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Feedback"
                                name="./feedback"
                                useFixedInlineToolbar="{Boolean}true"
                                removeSingleParagraphContainer="{Boolean}true"
                                value="É muito bom saber que podemos contar com você! O seu feedback é muito importante para nós.">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic" />
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-" />
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                    ref="justify" />
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists" />
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </Feedback>
                        </items>
                    </tab-textos>
                    <tab-imagem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Imagem"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <altImagem
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="ALT imagem"
                                name="./altImagem"
                                value="#" />
                            <imageDesktopWebp
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                autoStart="{Boolean}false"
                                class="cq-droptarget"
                                fieldLabel="Imagem Desktop WEBP (1440x700)"
                                fileNameParameter="./imageDesktopName"
                                fileReferenceParameter="./imageDesktopReference"
                                mimeTypes="[image/webp]"
                                multiple="{Boolean}false"
                                name="./imageDesktopWebp"
                                title="Imagem Desktop WEBP"
                                uploadUrl="${suffix.path}"
                                useHTML5="{Boolean}true" />
                        </items>
                    </tab-imagem>
                    <tab-form-settings
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tab-form-tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                                <items jcr:primaryType="nt:unstructured">
                                    <tab-botao
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Botão Enviar"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items
                                            jcr:primaryType="nt:unstructured">
                                            <finishButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Texto"
                                                name="./finishButtonText"
                                                value="" />
                                            <finishButtonTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="Atributo Title"
                                                name="./finishButtonTitle"
                                                value="#" />
                                            <finishButtonDataTrack
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="finishButton Data Track"
                                                name="./finishButtonDataTrack"
                                                value="#" />
                                            <finishButtonIconPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                deleteHint="{Boolean}true"
                                                fieldLabel="Posição do icone"
                                                name="./finishButtonIconPosition"
                                                vertical="{Boolean}false">
                                                <items
                                                    jcr:primaryType="nt:unstructured">
                                                    <branco
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Justificado"
                                                        value="justified" />
                                                    <cinza
                                                        jcr:primaryType="nt:unstructured"
                                                        checked="{Boolean}true"
                                                        text="Esquerda"
                                                        value="left" />
                                                    <azul
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Direita"
                                                        value="right" />
                                                </items>
                                            </finishButtonIconPosition>
                                            <finishButtonIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Imagem em .SGV"
                                                fieldLabel="Icone do Botão"
                                                fileNameParameter="./finishButtonIconNameParameter"
                                                fileReferenceParameter="./finishButtonIconReferenceParameter"
                                                mimeTypes="[image/svg]"
                                                multiple="{Boolean}false"
                                                name="./finishButtonIcon"
                                                title="icone do Botão (opcional)"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true" />
                                        </items>
                                    </tab-botao>
                                    <tab-captcha
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Captcha"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <Api
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                autofocus="{Boolean}true"
                                                fieldLabel="API - Endpoint"
                                                maxlength="{Long}200"
                                                name="./endPointApi"
                                                value="API - Endpoint" />
                                            <ChaveRecaptcha
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                autofocus="{Boolean}true"
                                                fieldLabel="Chave Recaptcha"
                                                maxlength="{Long}50"
                                                name="./chaveRecaptcha"
                                                value="6LfBpYkfAAAAAE3llrrAHo8OiCoxpQvX1pFq0YVi" />
                                        </items>
                                    </tab-captcha>
                                </items>
                            </tab-form-tabs>
                        </items>
                    </tab-form-settings>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="ID da Seção"
                                name="./sectionID"
                                value="" />
                            <tagPalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag da Palavra destaque"
                                name="./tagPalavraDestaque">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagPalavraDestaque>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do titulo"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagTitulo>
                        </items>
                    </tab-seo>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>