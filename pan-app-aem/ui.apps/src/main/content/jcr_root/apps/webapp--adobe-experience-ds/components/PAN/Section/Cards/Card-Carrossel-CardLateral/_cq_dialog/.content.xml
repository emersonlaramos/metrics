<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Card-Carrossel-CardLateral"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-variantes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variantes"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <HideButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Oculta o Mahoe-Button-1 do Componente"
                                text="Ocultar Button-1?"
                                uncheckedValue="{Boolean}true"
                                checked="{Boolean}false"
                                value="{Boolean}false"
                                name="./hideButton1" />
                            <HideButton2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Oculta o Mahoe-Button-2 do Componente"
                                text="Ocultar Button-2?"
                                uncheckedValue="{Boolean}true"
                                checked="{Boolean}false"
                                value="{Boolean}false"
                                name="./hideButton2" />
                            <HideLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldDescription="Oculta o Mahoe-Link do Componente"
                                text="Ocultar Mahoe-Link?"
                                uncheckedValue="{Boolean}true"
                                checked="{Boolean}false"
                                value="{Boolean}false"
                                name="./hideLink" />
                            <colorVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Cores de fundo"
                                name="./colorVariant"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Branco"
                                        value="bg-branco" />
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        text="Cinza"
                                        value="bg-cinza" />
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        text="Azul"
                                        value="bg-azul" />
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        text="Preto"
                                        value="bg-preto" />
                                </items>
                            </colorVariant>
                            <style-card
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Estilo do Card"
                                name="./styleCard"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <cardSolid
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Fundo Solido"
                                        value="" />
                                    <cardOutline
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Fundo Transparente com Borda"
                                        value="lined" />
                                </items>
                            </style-card>
                        </items>
                    </tab-variantes>
                    <tab-textos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Textos"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <PalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="Palavra Destaque"
                                name="./palavraDestaque"
                                value="Palavra destaque" />
                            <Titulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="Titulo"
                                name="./titulo"
                                value="Título com max 3 linhas aqui!" />
                            <Disclaimer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Disclaimer"
                                name="./disclaimer"
                                removeSingleParagraphContainer="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"
                                value="Disclaimer">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic" />
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-" />
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink" />
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                    ref="justify" />
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists" />
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </Disclaimer>
                            <Paragrafo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Parágrafo"
                                name="./paragrafo"
                                removeSingleParagraphContainer="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"
                                value="">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                        jcr:primaryType="nt:unstructured"
                                        features="bold,italic" />
                                    <justify
                                        jcr:primaryType="nt:unstructured"
                                        features="-" />
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="modifylink,unlink" />
                                    <lists
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                    <tracklinks
                                        jcr:primaryType="nt:unstructured"
                                        features="*" />
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright,justify#justifyjustify]"
                                                    ref="justify" />
                                                <lists
                                                    jcr:primaryType="nt:unstructured"
                                                    items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                    ref="lists" />
                                            </popovers>
                                        </inline>
                                    </cui>
                                </uiSettings>
                            </Paragrafo>
                        </items>
                    </tab-textos>
                    <tab-cards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Cards Carrossel"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <presets
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <list
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Insira os Cards">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./cardCarrosselLateral">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <IconeCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                            autoStart="{Boolean}false"
                                                            class="cq-droptarget"
                                                            fieldLabel="Icone do Card (32 x 32) -- .SVG (Opicional)"
                                                            fileNameParameter="./iconCardNameParameterSvg"
                                                            fileReferenceParameter="./iconCardReferenceSvg"
                                                            mimeTypes="[image/svg]"
                                                            multiple="{Boolean}false"
                                                            name="./iconCard"
                                                            title="icone do Card (opcional)"
                                                            uploadUrl="${suffix.path}"
                                                            useHTML5="{Boolean}true" />
                                                        <altIcon
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Texto Alternativo do icone"
                                                            name="./altIcon"
                                                            required="true"
                                                            value="" />
                                                        <ImageCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                            autoStart="{Boolean}false"
                                                            class="cq-droptarget"
                                                            fieldLabel="Imagem de fundo do card -- .WEBP (opicional)"
                                                            fileNameParameter="./imageCardNameParameter"
                                                            fileReferenceParameter="./imageCardReference"
                                                            mimeTypes="[image/webp]"
                                                            multiple="{Boolean}false"
                                                            name="./imageCard"
                                                            title="imagem de fundo do Card (opcional)"
                                                            uploadUrl="${suffix.path}"
                                                            useHTML5="{Boolean}true" />
                                                        <TituloCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Titulo"
                                                            name="./tituloCard"
                                                            value="Título aqui!" />
                                                        <tagTituloCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Tag do titulo do card"
                                                            name="./tagTituloCard">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <p
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="P"
                                                                    selected="true"
                                                                    value="p" />
                                                                <h1
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H1"
                                                                    value="h1" />
                                                                <h2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H2"
                                                                    value="h2" />
                                                                <h3
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H3"
                                                                    value="h3" />
                                                                <h4
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H4"
                                                                    value="h4" />
                                                                <h5
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H5"
                                                                    value="h5" />
                                                                <h6
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H6"
                                                                    value="h6" />
                                                            </items>
                                                        </tagTituloCard>
                                                        <TextoCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Texto"
                                                            name="./textoCard"
                                                            value="" />
                                                        <tagTextoCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Tag do Texto do Card"
                                                            name="./tagTextoCard">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <p
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="P"
                                                                    selected="true"
                                                                    value="p" />
                                                                <h1
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H1"
                                                                    value="h1" />
                                                                <h2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H2"
                                                                    value="h2" />
                                                                <h3
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H3"
                                                                    value="h3" />
                                                                <h4
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H4"
                                                                    value="h4" />
                                                                <h5
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H5"
                                                                    value="h5" />
                                                                <h6
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="H6"
                                                                    value="h6" />
                                                            </items>
                                                        </tagTextoCard>
                                                        <HideTextCheckbox
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            fieldDescription="Ocultar esse texto na versão Desktop"
                                                            name="./hideTextCheckbox"
                                                            text="Ocultar esse texto na versão Desktop ?"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true" />
                                                        <HrefCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            autocomplete="off"
                                                            fieldDescription="ID seção = #ID_DA_SECAO"
                                                            fieldLabel="CTA Link ou ID da seção para scroll (Não utilizar com bg-image)"
                                                            name="./hrefCard"
                                                            value="" />
                                                        <TargetCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="CTA Link Target (no caso de link)"
                                                            name="./ctaTargetCard">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <self
                                                                    jcr:primaryType="nt:unstructured"
                                                                    selected="{Boolean}true"
                                                                    text="Mesma Página"
                                                                    value="_self" />
                                                                <blank
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Nova Aba"
                                                                    value="_blank" />
                                                            </items>
                                                        </TargetCard>
                                                        <cardDataTrack
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Data-track do card (no caso de link)"
                                                            name="./dataTrackCard"
                                                            value="" />
                                                        <IconeLink2
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                            autoStart="{Boolean}false"
                                                            class="cq-droptarget"
                                                            fieldLabel="Icone do link com bg-image -- .SVG (opcional)"
                                                            fileNameParameter="./iconCardNameParameterSvg2"
                                                            fileReferenceParameter="./iconCardReferenceSvg2"
                                                            mimeTypes="[image/svg]"
                                                            multiple="{Boolean}false"
                                                            name="./iconCard2"
                                                            title="icone do link (opcional)"
                                                            uploadUrl="${suffix.path}"
                                                            useHTML5="{Boolean}true" />
                                                        <linkApoio
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            autofocus="{Boolean}true"
                                                            fieldLabel="Texto do link com bg-image"
                                                            name="./textApoio"
                                                            value="" />
                                                        <DataTrack
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Data Track do link com bg-image"
                                                            name="./LinkDataTrack"
                                                            value="" />
                                                        <HrefLink
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            autocomplete="off"
                                                            fieldLabel="Url do link com bg-image"
                                                            name="./linkHref"
                                                            value="" />

                                                    </items>
                                                </field>
                                            </list>
                                        </items>
                                    </column>
                                </items>
                            </presets>
                        </items>
                    </tab-cards>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                autofocus="{Boolean}true"
                                fieldLabel="ID da Seção"
                                name="./sectionID"
                                value="" />
                            <tagPalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag da Palavra destaque"
                                name="./tagPalavraDestaque">
                                <items jcr:primaryType="nt:unstructured">
                                    <p
                                        jcr:primaryType="nt:unstructured"
                                        text="P"
                                        selected="true"
                                        value="p" />
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagPalavraDestaque>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do titulo"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <p
                                        jcr:primaryType="nt:unstructured"
                                        text="P"
                                        value="p" />
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        selected="true"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagTitulo>
                            <tagParagrafo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do Paragrafo"
                                name="./tagParagrafo">
                                <items jcr:primaryType="nt:unstructured">
                                    <p
                                        jcr:primaryType="nt:unstructured"
                                        text="P"
                                        selected="true"
                                        value="p" />
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagParagrafo>
                        </items>
                    </tab-seo>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>