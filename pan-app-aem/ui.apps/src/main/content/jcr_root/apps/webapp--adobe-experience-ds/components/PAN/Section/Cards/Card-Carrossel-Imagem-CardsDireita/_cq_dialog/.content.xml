<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Propriedades Card-Carrossel-Imagem-CardsDireita"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <Tabs jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab-variantes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variante"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <positionVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                vertical="{Boolean}false"
                                fieldLabel="Posição da Imagem"
                                name="./positionVariant">
                                <items jcr:primaryType="nt:unstructured">
                                    <esquerda
                                        jcr:primaryType="nt:unstructured"
                                        text="Imagem na Esquerda"
                                        value="esquerda" />
                                    <direita
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Imagem na Direita"
                                        value="direita" />
                                </items>
                            </positionVariant>
                            <colorVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                fieldLabel="Cores de fundo"
                                name="./colorVariant"
                                deleteHint="{Boolean}true"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Branco"
                                        value="bg-branco" />
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Cinza"
                                        value="bg-cinza" />
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        text="Azul"
                                        value="bg-azul" />
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        text="Preto"
                                        value="bg-preto" />
                                </items>
                            </colorVariant>
                        </items>
                    </tab-variantes>
                    <tab-textos
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Textos"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <PalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Palavra Destaque"
                                name="./palavraDestaque"
                                value="{String}Palavra de destaque"
                                autocomplete="off"
                                autofocus="{Boolean}true" />
                            <TituloDesktop
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Titulo Desktop"
                                name="./tituloDesk"
                                value="{String}Exemplo de título com no máximo 3 linhas aqui!"
                                autocomplete="off" />
                            <TituloMobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Titulo Mobile"
                                name="./tituloMob"
                                value="{String}Título com 1 linha aqui!"
                                autocomplete="off" />
                        </items>
                    </tab-textos>
                    <tab-cards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Cards"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <presets
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <list
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldLabel="Insira os Slides"
                                                composite="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./imagemCardsDireita">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <TextoCard
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            autocomplete="off"
                                                            fieldLabel="Texto do Card"
                                                            name="./textCard"
                                                            value="Lorem ipsum dolor sit" />
                                                    </items>
                                                </field>
                                            </list>
                                        </items>
                                    </column>
                                </items>
                            </presets>
                        </items>
                    </tab-cards>
                    <tab-imagem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Imagem"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <altImagem
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                autocomplete="off"
                                fieldLabel="ALT imagem"
                                name="./altImagem"
                                value="#" />
                            <imagemWebp
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                autoStart="{Boolean}false"
                                class="cq-droptarget"
                                fieldLabel="Imagem WEBP (710 x 688)"
                                fieldDescription="Resolução referente a 6 colunas"
                                fileNameParameter="./imageName"
                                fileReferenceParameter="./imageReference"
                                mimeTypes="[image/webp]"
                                multiple="{Boolean}false"
                                name="./imageWebp"
                                title="Imagem WEBP"
                                uploadUrl="${suffix.path}"
                                useHTML5="{Boolean}true" />
                        </items>
                    </tab-imagem>
                    <tab-seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <sectionID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="ID da Seção"
                                name="./sectionID"
                                value="{String}"
                                autocomplete="off"
                                autofocus="{Boolean}true" />
                            <tagPalavraDestaque
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag da Palavra destaque"
                                name="./tagPalavraDestaque">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"
                                        selected="true" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagPalavraDestaque>
                            <tagTitulo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Tag do titulo"
                                name="./tagTitulo">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"
                                        selected="true" />
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2" />
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3" />
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4" />
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5" />
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6" />
                                </items>
                            </tagTitulo>
                        </items>
                    </tab-seo>
                </items>
            </Tabs>
        </items>
    </content>
</jcr:root>