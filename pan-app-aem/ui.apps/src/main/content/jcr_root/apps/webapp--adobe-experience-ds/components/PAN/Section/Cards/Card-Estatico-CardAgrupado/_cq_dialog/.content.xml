                    <tab-variantes
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variantes"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <colorVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Cores de fundo"
                                name="./colorVariant"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Branco"
                                        value="bg-branco" />
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        text="Cinza"
                                        value="bg-cinza" />
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        text="Azul"
                                        value="bg-azul" />
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        text="Preto"
                                        value="bg-preto" />
                                </items>
                            </colorVariant>
                            <positionVariant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                deleteHint="{Boolean}true"
                                fieldLabel="Posição dos Cards"
                                name="./positionVariant"
                                vertical="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <direita
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Cards na Direita"
                                        value="direita" />
                                    <esquerda
                                        jcr:primaryType="nt:unstructured"
                                        text="Cards na Esquerda"
                                        value="esquerda" />
                                </items>
                            </positionVariant>
                        </items>
                    </tab-variantes>
                    <tab-textos
