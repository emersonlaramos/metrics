                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Imagem em .SVG"
                                                fieldLabel="Icone do Card (32 x 32)"
                                                fileNameParameter="./iconCard1NameParameterSvg"
                                                fileReferenceParameter="./iconCard1ReferenceSvg"
                                                mimeTypes="[image/svg]"
                                                multiple="{Boolean}false"
                                                name="./iconCard1"
                                                title="Imagem Mobile SVG"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true" />
                                            <altIcon1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                autocomplete="off"
                                                fieldLabel="ALT Icon"
                                                name="./altIcon1"
                                                value="#" />
